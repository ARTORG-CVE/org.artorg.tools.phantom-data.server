<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <bottom>
      <AnchorPane BorderPane.alignment="CENTER">
         <children>
            <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="10.0">
               <children>
                  <Label fx:id="progressLabel" maxWidth="1.7976931348623157E308" prefHeight="20.0" VBox.vgrow="ALWAYS">
                     <font>
                        <Font size="10.0" />
                     </font></Label>
                  <ProgressBar fx:id="progressBar" maxWidth="1.7976931348623157E308" progress="0.0" VBox.vgrow="ALWAYS" />
               </children>
            </VBox>
         </children>
      </AnchorPane>
   </bottom>
   <left>
      <ImageView fitHeight="181.0" fitWidth="200.0" BorderPane.alignment="CENTER">
         <image>
            <Image url="@../img/startup.png" />
         </image>
      </ImageView>
   </left>
   <right>
      <VBox alignment="CENTER_RIGHT" prefHeight="200.0" prefWidth="100.0" spacing="50.0" BorderPane.alignment="CENTER">
         <children>
            <ImageView>
               <image>
                  <Image url="@../img/artorgLogo.png" />
               </image>
            </ImageView>
            <ImageView fitWidth="166.0">
               <image>
                  <Image url="@../img/inselLogo.png" />
               </image>
            </ImageView>
         </children>
      </VBox>
   </right>
   <center>
      <AnchorPane minWidth="30.0" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </center>
   <top>
      <HBox nodeOrientation="RIGHT_TO_LEFT" spacing="10.0" BorderPane.alignment="TOP_RIGHT">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" onMouseClicked="#close" prefHeight="15.0" prefWidth="15.0" text="x" textAlignment="CENTER" />
            <Label alignment="CENTER" contentDisplay="CENTER" onMouseClicked="#minimize" prefHeight="15.0" prefWidth="15.0" text="_" />
         </children>
      </HBox>
   </top>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</BorderPane>
